Câu select này của tôi ra dữ liệu thế này, nó đang nhiều hơn so với thực tế
SELECT a.*,
b.PTYP   NSTEP_CHECK,
b.PFLAG1,
b.PDESC,
c.nstep,
d.ptyp STEP_CHECK,
e.NEWGRP ROUTE_GRP
FROM route_step a
left JOIN route_step_nstep b ON b.step = a.step
LEFT JOIN route_step_nstep_d c ON c.step = b.step
LEFT JOIN route_step_check d ON d.step = a.STEP
LEFT JOIN route_grp e ON e.route = a.route AND e.grp = a.grp
WHERE a.step ='MKBAWWQC' ORDER BY a.ridx;

